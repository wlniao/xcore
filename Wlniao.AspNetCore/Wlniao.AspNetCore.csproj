<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Authors>XieChaoyi</Authors>
    <Nullable>enable</Nullable>
    <Version>8.0.14-beta1</Version>
    <TargetFramework>net8.0</TargetFramework>
    <PackageId>Wlniao.AspNetCore</PackageId>
    <PackageTags>Wlniao;XCore;CoreCLR;XServer</PackageTags>
    <Description>Wlniao XCore.AspNetCore for CoreCLR Framework</Description>
    <AssemblyName>Wlniao.XCore.AspNetCore</AssemblyName>
    <Copyright>Copyright 2022 XieChaoyi</Copyright>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageUrl>https://static.wlniao.com/wlniao/logo.png</PackageUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <RootNamespace>Wlniao</RootNamespace>
    <RepositoryUrl>https://github.com/wlniao/xcore</RepositoryUrl>
    <PackageIcon>wlniao.png</PackageIcon>
    <PackageOutputPath>C:\Users\Administrator\OneDrive\Path\packages</PackageOutputPath>
    <PackageProjectUrl>https://github.com/wlniao/xcore</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <IncludeSymbols>False</IncludeSymbols>
    <EnableNETAnalyzers>False</EnableNETAnalyzers>
    <NoWarn>CS8602;CS8618;CS8625</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="8.0.2" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Razor.RuntimeCompilation" Version="8.0.2" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="8.0.0" />
    <PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="8.0.0" />
    <PackageReference Include="Microsoft.OpenApi" Version="1.6.13" />
    <PackageReference Include="Swashbuckle.AspNetCore.SwaggerGen" Version="6.5.0" />
    <PackageReference Include="Wlniao.XCore" Version="8.0.13" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\wlniao.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
